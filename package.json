{
  "name": "cookiecutter",
  "displayName": "Cookiecutter",
  "description": "Just launch cookiecutter for project creation",
  "version": "0.1.2",
  "publisher": "gxiao",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e415e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.11.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.cookiecutter"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.cookiecutter",
        "title": "Cookiecutter ..."
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "extension.cookiecutter"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "eslint": "^4.18.2",
    "mocha": "^6.1.4",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "js-yaml": "^3.13.1"
  },
  "homepage": "https://github.com/gongxiao/vscode-cookiecutter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gongxiao/vscode-cookiecutter.git"
  },
  "bugs": {
    "url": "https://github.com/gongxiao/vscode-cookiecutter/issues"
  },
  "license": "MIT",
  "keywords": [
    "git",
    "vscode",
    "mergetools",
    "difftools",
    "merge conflict",
    "conflict",
    "extensions"
  ],
  "__metadata": {
    "publisherDisplayName": "Gong Xiao"
  }
}
